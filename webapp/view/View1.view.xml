<mvc:View controllerName="ovly.random-cat.controller.View1" xmlns:s="sap.m.semantic" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns="sap.m">
	<Shell id="shell">
		<SplitContainer id="app">
			<masterPages>
				<s:MasterPage title="Random Cats">
					<s:content>
						<List id="cat-list" includeItemInSelection="false" >
							<items>
					
							</items>
						</List>
					</s:content>
					<s:customFooterContent>
						<Button icon="sap-icon://delete" press="onEmpty" />
					</s:customFooterContent>
					<s:addAction>
						<s:AddAction press="onAdd" />
					</s:addAction>
				</s:MasterPage>
			</masterPages>
			<detailPages>
				<s:DetailPage id="detail-page" title="" showFooter="false">
					<s:content>
						<HBox justifyContent="Center">
							<Image id="cat-image" width="300px" src=""/>
						</HBox>
					</s:content>
				</s:DetailPage>
			</detailPages>
		</SplitContainer>
	</Shell>
</mvc:View>